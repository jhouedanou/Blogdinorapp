{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --omit=dev",
  "framework": null,
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server/**"],
        "tscOptions": {
          "sourceMap": false
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public,max-age=31536000,immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
