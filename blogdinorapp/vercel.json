{
  "version": 2,
  "buildCommand": "npm run generate",
  "outputDirectory": ".output/public",
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "dest": "/_nuxt/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/images/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
