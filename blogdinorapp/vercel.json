{
  "version": 2,
  "framework": "nuxt",
  "buildCommand": "npm run build",
  "outputDirectory": ".output/public",
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/images/(.*)",
      "headers": {
        "cache-control": "public, max-age=604800"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
