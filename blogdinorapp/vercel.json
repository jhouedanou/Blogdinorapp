{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --omit=dev",
  "framework": "nuxt",
  "regions": ["cdg1"],
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server/**"],
        "tscOptions": {
          "sourceMap": false
        }
      }
    }
  ]
}
