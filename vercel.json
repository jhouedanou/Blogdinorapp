{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".output",
  "routes": [
    {
      "src": "/_nuxt/(.*)",
      "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}